P = [1299721,1300073,1300421,1300751,1299743,1300097,1300423,1300769,1299763,1300111,1300433,1300771,1299791,1300127,1300451,1300781,1299811,1300129,1300457,1300813,1299817,1300133,1300463,1300829,1299821,1300139,1300471,1300837,1299827,1300141,1300477,1300841,1299833,1300147,1300487,1300843,1299841,1300153,1300501,1300907,1299853,1300181,1300511,1300921,1299869,1300193,1300553,1300927,1299877,1300199,1300571,1300931,1299887,1300237,1300573,1300963,1299899,1300253,1300583,1300967,1299917,1300283,1300597,1300979,1299919,1300289,1300609,1300997,1299941,1300297,1300613,1301011,1299953,1300307,1300633,1301017,1299979,1300309,1300639,1301021,1299989,1300319,1300669,1301023,1300021,1300333,1300681,1301033,1300027,1300339,1300709,1301057,1300031,1300367,1300711,1301077,1300051,1300391,1300727,1301081]
A = None;
def f(x,mod):
	r = 0;
	for i in xrange(len(A)):
		r = (r*x + A[i])%mod;
	return r;	
	
def main():
	global A;
	n,k = map(int,raw_input().split());
	A = ["" for i in xrange(n + 1)];
	ctr = 0;
	for i in xrange(n + 1):
		A[i] = raw_input();
		ctr += A[i][0] == '?';
	if k == 0:
		if A[0][0] == '?':
			print "YES" if (n - ctr) & 1 else "NO";
			return;
		else:
			print "NO" if int(A[0]) else "YES";
			return;
	else:
		if ctr :
			print "YES" if n & 1 else "NO";
			return;
		A = [int(x) for x in A];
		A.reverse();
		for p in P:
			if f(k,p):
				print "NO";
				return;
		print "YES";	
if __name__ == "__main__":
	main();
	
